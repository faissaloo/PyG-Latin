obj player {
    event create
    {
        self.y=0
        self.x=0
    }
    event step
    {

        if keyboard_check_pressed(97)
        {
            self.x-=1
        }
        else if keyboard_check_pressed(100)
        {
            self.x+=1
        }
        else if keyboard_check_pressed(119)
        {
            self.y-=1
        }
        else if keyboard_check_pressed(115)
        {
            self.y+=1
        }

    }
    event draw
    {
        a=1
        draw_point(self.y,self.x)
    }
}

room rm1 {
    room_speed=60
    room_width=24
    room_height=88
    instance_create(10,10,player)
}
current_room=rm1()
