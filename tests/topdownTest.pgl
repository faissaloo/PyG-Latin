obj player {
    event create
    {
        y=0
        x=0
    }
    event step
    {
        if 1 + 1
        {
            x-=1
        }
        if keyboard_check_pressed(100)
        {
            x+=1
        }
    }
    event draw
    {
        draw_point(y,x)
    }
}

room rm1 {
    room_speed=60
    room_width=24
    room_height=88
    instance_create(10,10,player)
}
current_rm=rm1
